//
// All profiles begin with this data and overwrite their own
//
Default
	Skill = 100
	Aggression = 100
	ReactionTime = 0.00001
	AttackDelay = 0
	Teamwork = 75
	AimFocusInitial = 0.0005			// initial focus spread in degrees (from desired center)
	AimFocusDecay = 0.00010				// how much focus shrinks per second (.25 = 25% of size after 1 sec)
	AimFocusOffsetScale = 0.00005		// controls accuracy when tracking to target (0 == perfect, should always be < 1)
	AimfocusInterval = 0.00005			// how often focus is adjusted (smaller intervals means better movement tracking)
	WeaponPreference = none
	Cost = 0
	Difficulty = Elite
	VoicePitch = 100
	Skin = 0
	LookAngleMaxAccelNormal = 2000000.0
	LookAngleStiffnessNormal = 1000.0
	LookAngleDampingNormal = 25.0
	LookAngleMaxAccelAttacking = 30000000.0
	LookAngleStiffnessAttacking = 1000.0
	LookAngleDampingAttacking = 30.0
End

